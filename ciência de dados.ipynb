   "source": [
    "arquivo = pd.read_csv(\"dados_dengue.csv\")\n",
    "arquivo.head()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 12,
   "id": "507a539a",
   "metadata": {},
   "outputs": [],
   "source": [
    "anos = arquivo[['ano']]\n",
    "casos = arquivo[['casos']]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 14,
   "id": "047c50b9",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Casos previstos para 2018 -> 330\n"
     ]
    }
   ],
   "source": [
    "# Mineração\n",
    "\n",
    "regr = LinearRegression()\n",
    "regr.fit(X=anos, y=casos)\n",
    "\n",
    "ano_futuro = [[2018]]\n",
    "casos_2018 = regr.predict(ano_futuro)\n",
    "\n",
    "print('Casos previstos para 2018 ->', int(casos_2018))"
   ]
  },
